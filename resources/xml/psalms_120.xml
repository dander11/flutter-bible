<?xml version="1.0" encoding="utf-8"?> 
<crossway-bible>
	<book title="Psalms" num="18">
		<marker class="begin-verse" mid="v19120001">
		</marker>
		<chapter num="120">
			<heading
				>
				Deliver Me, O 
				<span class="divine-name">
					Lord
				</span>
			</heading>
			<subheading class="psalm-title">
				A Song of 
				<crossref let="m" cid="c19120001.1">
				</crossref>
				Ascents.
			</subheading>
			<begin-block-indent
				>
			</begin-block-indent>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="1">
				<begin-line
					>
				</begin-line>
				In my distress I called to the 
				<span class="divine-name">
					Lord
				</span>
				,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				and he answered me.
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19120002">
			</marker>
			<verse num="2">
				<begin-line
					>
				</begin-line>
				Deliver me, O 
				<span class="divine-name">
					Lord
				</span>
				,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				from lying lips,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				from a deceitful tongue.
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v19120003">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="3">
				<begin-line
					>
				</begin-line>
				What shall be given to you,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				<crossref let="n" cid="c19120003.1">
				</crossref>
				and what more shall be done to you,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				you deceitful tongue?
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19120004">
			</marker>
			<verse num="4">
				<begin-line
					>
				</begin-line>
				<crossref let="o" cid="c19120004.1">
				</crossref>
				A warrior's 
				<crossref let="p" cid="c19120004.2">
				</crossref>
				sharp arrows,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				with glowing 
				<crossref let="q" cid="c19120004.3">
				</crossref>
				coals of the broom tree!
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v19120005">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="5">
				<begin-line
					>
				</begin-line>
				Woe to me, that I sojourn in 
				<crossref let="r" cid="c19120005.1">
				</crossref>
				Meshech,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				that I dwell among 
				<crossref let="s" cid="c19120005.2">
				</crossref>
				the tents of 
				<crossref let="t" cid="c19120005.3">
				</crossref>
				Kedar!
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19120006">
			</marker>
			<verse num="6">
				<begin-line
					>
				</begin-line>
				Too long have I had my dwelling
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				among those who hate peace.
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19120007">
			</marker>
			<verse num="7">
				<begin-line
					>
				</begin-line>
				<crossref let="u" cid="c19120007.1">
				</crossref>
				I am for peace,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				but when I speak, they are for war!
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<end-block-indent
				>
			</end-block-indent>
