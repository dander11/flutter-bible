<?xml version="1.0" encoding="utf-8"?> 
<crossway-bible>
	<book title="Psalms" num="18">
		<marker class="begin-verse" mid="v19096001">
		</marker>
		<chapter num="96">
			<heading
				>
				Worship in the Splendor of Holiness
			</heading>
			<begin-block-indent
				>
			</begin-block-indent>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="1">
				<begin-line
					>
				</begin-line>
				<crossref let="a" cid="c19096001.1">
				</crossref>
				Oh sing to the 
				<span class="divine-name">
					Lord
				</span>
				<crossref let="b" cid="c19096001.2">
				</crossref>
				a new song;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				sing to the 
				<span class="divine-name">
					Lord
				</span>
				, all the earth!
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19096002">
			</marker>
			<verse num="2">
				<begin-line
					>
				</begin-line>
				Sing to the 
				<span class="divine-name">
					Lord
				</span>
				, bless his name;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				<crossref let="c" cid="c19096002.1">
				</crossref>
				tell of his salvation from day to day.
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19096003">
			</marker>
			<verse num="3">
				<begin-line
					>
				</begin-line>
				Declare his glory among the nations,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				his marvelous works among all the peoples!
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19096004">
			</marker>
			<verse num="4">
				<begin-line
					>
				</begin-line>
				For 
				<crossref let="d" cid="c19096004.1">
				</crossref>
				great is the 
				<span class="divine-name">
					Lord
				</span>
				, and 
				<crossref let="e" cid="c19096004.2">
				</crossref>
				greatly to be praised;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				he is to be feared above 
				<crossref let="f" cid="c19096004.3">
				</crossref>
				all gods.
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19096005">
			</marker>
			<verse num="5">
				<begin-line
					>
				</begin-line>
				For all the gods of the peoples are worthless idols,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				but the 
				<span class="divine-name">
					Lord
				</span>
				<crossref let="g" cid="c19096005.1">
				</crossref>
				made the heavens.
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19096006">
			</marker>
			<verse num="6">
				<begin-line
					>
				</begin-line>
				Splendor and majesty are before him;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				<crossref let="h" cid="c19096006.1">
				</crossref>
				strength and beauty are in his sanctuary.
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v19096007">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="7">
				<begin-line
					>
				</begin-line>
				Ascribe to the 
				<span class="divine-name">
					Lord
				</span>
				, O 
				<crossref let="i" cid="c19096007.1">
				</crossref>
				families of the peoples,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				<crossref let="j" cid="c19096007.2">
				</crossref>
				ascribe to the 
				<span class="divine-name">
					Lord
				</span>
				 glory and strength!
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19096008">
			</marker>
			<verse num="8">
				<begin-line
					>
				</begin-line>
				Ascribe to the 
				<span class="divine-name">
					Lord
				</span>
				<crossref let="k" cid="c19096008.1">
				</crossref>
				the glory due his name;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				bring 
				<crossref let="l" cid="c19096008.2">
				</crossref>
				an offering, and 
				<crossref let="m" cid="c19096008.3">
				</crossref>
				come into his courts!
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19096009">
			</marker>
			<verse num="9">
				<begin-line
					>
				</begin-line>
				Worship the 
				<span class="divine-name">
					Lord
				</span>
				 in 
				<crossref let="n" cid="c19096009.1">
				</crossref>
				the splendor of holiness;
				<note nid="n19096009.1">
				</note>
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				<crossref let="o" cid="c19096009.2">
				</crossref>
				tremble before him, all the earth!
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v19096010">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="10">
				<begin-line
					>
				</begin-line>
				Say among the nations, 
				<crossref let="p" cid="c19096010.1">
				</crossref>
				<q class="begin-double" qid="" from="" to="">
				</q>
				The 
				<span class="divine-name">
					Lord
				</span>
				 reigns!
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				Yes, the world is established; it shall never be moved;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				he will 
				<crossref let="q" cid="c19096010.2">
				</crossref>
				judge the peoples with equity.
				<q class="end-double" qid="" from="" to="">
				</q>
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v19096011">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="11">
				<begin-line
					>
				</begin-line>
				Let 
				<crossref let="r" cid="c19096011.1">
				</crossref>
				the heavens be glad, and let 
				<crossref let="s" cid="c19096011.2">
				</crossref>
				the earth rejoice;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				let 
				<crossref let="t" cid="c19096011.3">
				</crossref>
				the sea roar, and all that fills it;
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19096012">
			</marker>
			<verse num="12">
				<begin-line class="indent">
				</begin-line>
				let 
				<crossref let="u" cid="c19096012.1">
				</crossref>
				the field exult, and everything in it!
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				Then shall all 
				<crossref let="v" cid="c19096012.2">
				</crossref>
				the trees of the forest sing for joy
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19096013">
			</marker>
			<verse num="13">
				<begin-line class="indent">
				</begin-line>
				before the 
				<span class="divine-name">
					Lord
				</span>
				, for he comes,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				for he comes 
				<crossref let="w" cid="c19096013.1">
				</crossref>
				to judge the earth.
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				He will judge the world in righteousness,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				and the peoples in his faithfulness.
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<end-block-indent
				>
			</end-block-indent>
		</chapter>
	</book>
</crossway-bible>


