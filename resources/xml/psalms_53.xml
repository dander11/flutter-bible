<?xml version="1.0" encoding="utf-8"?> 
<crossway-bible>
	<book title="Psalms" num="18">
		<marker class="begin-verse" mid="v19053001">
		</marker>
		<chapter num="53">
			<heading
				>
				There Is None Who Does Good
			</heading>
			<subheading class="psalm-title">
				To the choirmaster: according to 
				<crossref let="p" cid="c19053001.1">
				</crossref>
				Mahalath. A Maskil
				<note nid="n19053001.1">
				</note>
				 of David.
			</subheading>
			<begin-block-indent
				>
			</begin-block-indent>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="1">
				<begin-line
					>
				</begin-line>
				<crossref let="q" cid="c19053001.2">
				</crossref>
				The fool says in his heart, 
				<q class="begin-double" qid="" from="" to="">
				</q>
				There is no God.
				<q class="end-double" qid="" from="" to="">
				</q>
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				They are corrupt, doing abominable iniquity;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				there is none who does good.
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v19053002">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="2">
				<begin-line
					>
				</begin-line>
				God looks down from heaven
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				on the children of man
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				to see if there are any who understand,
				<note nid="n19053002.1">
				</note>
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				who seek after God.
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v19053003">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="3">
				<begin-line
					>
				</begin-line>
				They have all fallen away;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				together they have become corrupt;
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				there is none who does good,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				not even one.
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v19053004">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="4">
				<begin-line
					>
				</begin-line>
				Have those who work evil no knowledge,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				who eat up my people as they eat bread,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				and do not call upon God?
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v19053005">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="5">
				<begin-line
					>
				</begin-line>
				There they are, in great terror,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				<crossref let="r" cid="c19053005.1">
				</crossref>
				where there is no terror!
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				For God 
				<crossref let="s" cid="c19053005.2">
				</crossref>
				scatters the bones of him who encamps against you;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				you put them to shame, for God has rejected them.
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v19053006">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="6">
				<begin-line
					>
				</begin-line>
				Oh, that salvation for Israel would come out of Zion!
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				When God restores the fortunes of his people,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				let Jacob rejoice, let Israel be glad.
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<end-block-indent
				>
			</end-block-indent>
		</chapter>
	</book>
</crossway-bible>


