<?xml version="1.0" encoding="utf-8"?> 
<crossway-bible>
	<book title="Psalms" num="18">
		<marker class="begin-verse" mid="v19115001">
		</marker>
		<chapter num="115">
			<heading
				>
				To Your Name Give Glory
			</heading>
			<begin-block-indent
				>
			</begin-block-indent>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="1">
				<begin-line
					>
				</begin-line>
				<crossref let="a" cid="c19115001.1">
				</crossref>
				Not to us, O 
				<span class="divine-name">
					Lord
				</span>
				, not to us, but to your name give glory,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				<crossref let="b" cid="c19115001.2">
				</crossref>
				for the sake of your steadfast love and your faithfulness!
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v19115002">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="2">
				<begin-line
					>
				</begin-line>
				Why should the nations say,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				<crossref let="c" cid="c19115002.1">
				</crossref>
				<q class="begin-double" qid="" from="" to="">
				</q>
				Where is their God?
				<q class="end-double" qid="" from="" to="">
				</q>
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19115003">
			</marker>
			<verse num="3">
				<begin-line
					>
				</begin-line>
				<crossref let="d" cid="c19115003.1">
				</crossref>
				Our God is in the heavens;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				<crossref let="e" cid="c19115003.2">
				</crossref>
				he does all that he pleases.
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v19115004">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="4">
				<begin-line
					>
				</begin-line>
				<crossref let="f" cid="c19115004.1">
				</crossref>
				Their idols are silver and gold,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				<crossref let="g" cid="c19115004.2">
				</crossref>
				the work of human hands.
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19115005">
			</marker>
			<verse num="5">
				<begin-line
					>
				</begin-line>
				They have mouths, 
				<crossref let="h" cid="c19115005.1">
				</crossref>
				but do not speak;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				eyes, but do not see.
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19115006">
			</marker>
			<verse num="6">
				<begin-line
					>
				</begin-line>
				They have ears, but do not hear;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				noses, but do not smell.
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19115007">
			</marker>
			<verse num="7">
				<begin-line
					>
				</begin-line>
				They have hands, but do not feel;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				feet, but do not walk;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				and they do not make a sound in their throat.
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19115008">
			</marker>
			<verse num="8">
				<begin-line
					>
				</begin-line>
				<crossref let="i" cid="c19115008.1">
				</crossref>
				Those who make them become like them;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				so do all who trust in them.
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v19115009">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="9">
				<begin-line
					>
				</begin-line>
				O 
				<crossref let="j" cid="c19115009.1">
				</crossref>
				Israel,
				<note nid="n19115009.1">
				</note>
				<crossref let="k" cid="c19115009.2">
				</crossref>
				trust in the 
				<span class="divine-name">
					Lord
				</span>
				!
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				He is their 
				<crossref let="l" cid="c19115009.3">
				</crossref>
				help and their shield.
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19115010">
			</marker>
			<verse num="10">
				<begin-line
					>
				</begin-line>
				O 
				<crossref let="j" cid="c19115010.1">
				</crossref>
				house of Aaron, trust in the 
				<span class="divine-name">
					Lord
				</span>
				!
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				He is their help and 
				<crossref let="m" cid="c19115010.2">
				</crossref>
				their shield.
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19115011">
			</marker>
			<verse num="11">
				<begin-line
					>
				</begin-line>
				You 
				<crossref let="n" cid="c19115011.1">
				</crossref>
				who fear the 
				<span class="divine-name">
					Lord
				</span>
				, trust in the 
				<span class="divine-name">
					Lord
				</span>
				!
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				He is their help and their shield.
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v19115012">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="12">
				<begin-line
					>
				</begin-line>
				The 
				<span class="divine-name">
					Lord
				</span>
				 has remembered us; he will bless us;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				he will bless 
				<crossref let="o" cid="c19115012.1">
				</crossref>
				the house of Israel;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				he will bless 
				<crossref let="o" cid="c19115012.2">
				</crossref>
				the house of Aaron;
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19115013">
			</marker>
			<verse num="13">
				<begin-line
					>
				</begin-line>
				he will 
				<crossref let="p" cid="c19115013.1">
				</crossref>
				bless those who fear the 
				<span class="divine-name">
					Lord
				</span>
				,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				<crossref let="q" cid="c19115013.2">
				</crossref>
				both the small and the great.
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v19115014">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="14">
				<begin-line
					>
				</begin-line>
				May the 
				<span class="divine-name">
					Lord
				</span>
				<crossref let="r" cid="c19115014.1">
				</crossref>
				give you increase,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				you and your children!
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19115015">
			</marker>
			<verse num="15">
				<begin-line
					>
				</begin-line>
				May 
				<crossref let="s" cid="c19115015.1">
				</crossref>
				you be blessed by the 
				<span class="divine-name">
					Lord
				</span>
				,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				<crossref let="t" cid="c19115015.2">
				</crossref>
				who made heaven and earth!
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v19115016">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="16">
				<begin-line
					>
				</begin-line>
				The heavens are the 
				<span class="divine-name">
					Lord
				</span>
				's heavens,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				but the earth he has given to the children of man.
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19115017">
			</marker>
			<verse num="17">
				<begin-line
					>
				</begin-line>
				<crossref let="u" cid="c19115017.1">
				</crossref>
				The dead do not praise the 
				<span class="divine-name">
					Lord
				</span>
				,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				nor do any who go down into 
				<crossref let="v" cid="c19115017.2">
				</crossref>
				silence.
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19115018">
			</marker>
			<verse num="18">
				<begin-line
					>
				</begin-line>
				But 
				<crossref let="w" cid="c19115018.1">
				</crossref>
				we will bless the 
				<span class="divine-name">
					Lord
				</span>
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				from this time forth and forevermore.
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				<crossref let="x" cid="c19115018.2">
				</crossref>
				Praise the 
				<span class="divine-name">
					Lord
				</span>
				!
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<end-block-indent
				>
			</end-block-indent>
		</chapter>
	</book>
</crossway-bible>


