<?xml version="1.0" encoding="utf-8"?> 
<crossway-bible>
	<book title="Psalms" num="18">
		<marker class="begin-verse" mid="v19114001">
		</marker>
		<chapter num="114">
			<heading
				>
				Tremble at the Presence of the Lord
			</heading>
			<begin-block-indent
				>
			</begin-block-indent>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="1">
				<begin-line
					>
				</begin-line>
				When 
				<crossref let="p" cid="c19114001.1">
				</crossref>
				Israel went out from Egypt,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				the house of Jacob from 
				<crossref let="q" cid="c19114001.2">
				</crossref>
				a people of strange language,
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19114002">
			</marker>
			<verse num="2">
				<begin-line
					>
				</begin-line>
				Judah became his 
				<crossref let="r" cid="c19114002.1">
				</crossref>
				sanctuary,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				Israel his dominion.
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v19114003">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="3">
				<begin-line
					>
				</begin-line>
				<crossref let="s" cid="c19114003.1">
				</crossref>
				The sea looked and fled;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				<crossref let="t" cid="c19114003.2">
				</crossref>
				Jordan turned back.
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19114004">
			</marker>
			<verse num="4">
				<begin-line
					>
				</begin-line>
				<crossref let="u" cid="c19114004.1">
				</crossref>
				The mountains skipped like rams,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				the hills like lambs.
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v19114005">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="5">
				<begin-line
					>
				</begin-line>
				What 
				<crossref let="v" cid="c19114005.1">
				</crossref>
				ails you, O sea, that you flee?
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				O Jordan, that you turn back?
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19114006">
			</marker>
			<verse num="6">
				<begin-line
					>
				</begin-line>
				O mountains, that you skip like rams?
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				O hills, like lambs?
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v19114007">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="7">
				<begin-line
					>
				</begin-line>
				<crossref let="w" cid="c19114007.1">
				</crossref>
				Tremble, O earth, at the presence of the Lord,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				at the presence of the God of Jacob,
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19114008">
			</marker>
			<verse num="8">
				<begin-line
					>
				</begin-line>
				who turns 
				<crossref let="x" cid="c19114008.1">
				</crossref>
				the rock into 
				<crossref let="y" cid="c19114008.2">
				</crossref>
				a pool of water,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				<crossref let="z" cid="c19114008.3">
				</crossref>
				the flint into a spring of water.
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<end-block-indent
				>
			</end-block-indent>
		</chapter>
	</book>
</crossway-bible>


