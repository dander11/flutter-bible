<?xml version="1.0" encoding="utf-8"?> 
<crossway-bible>
	<book title="Psalms" num="18">
		<marker class="begin-verse" mid="v19097001">
		</marker>
		<chapter num="97">
			<heading
				>
				The 
				<span class="divine-name">
					Lord
				</span>
				 Reigns
			</heading>
			<begin-block-indent
				>
			</begin-block-indent>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="1">
				<begin-line
					>
				</begin-line>
				<crossref let="x" cid="c19097001.1">
				</crossref>
				The 
				<span class="divine-name">
					Lord
				</span>
				 reigns, 
				<crossref let="y" cid="c19097001.2">
				</crossref>
				let the earth rejoice;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				let the many 
				<crossref let="z" cid="c19097001.3">
				</crossref>
				coastlands be glad!
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19097002">
			</marker>
			<verse num="2">
				<begin-line
					>
				</begin-line>
				<crossref let="a" cid="c19097002.1">
				</crossref>
				Clouds and thick darkness are all around him;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				<crossref let="b" cid="c19097002.2">
				</crossref>
				righteousness and justice are the foundation of his throne.
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19097003">
			</marker>
			<verse num="3">
				<begin-line
					>
				</begin-line>
				<crossref let="c" cid="c19097003.1">
				</crossref>
				Fire goes before him
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				and burns up his adversaries all around.
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19097004">
			</marker>
			<verse num="4">
				<begin-line
					>
				</begin-line>
				His 
				<crossref let="d" cid="c19097004.1">
				</crossref>
				lightnings light up the world;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				the earth sees and 
				<crossref let="e" cid="c19097004.2">
				</crossref>
				trembles.
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19097005">
			</marker>
			<verse num="5">
				<begin-line
					>
				</begin-line>
				The mountains 
				<crossref let="f" cid="c19097005.1">
				</crossref>
				melt like 
				<crossref let="g" cid="c19097005.2">
				</crossref>
				wax before the 
				<span class="divine-name">
					Lord
				</span>
				,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				before 
				<crossref let="h" cid="c19097005.3">
				</crossref>
				the Lord of all the earth.
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v19097006">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="6">
				<begin-line
					>
				</begin-line>
				<crossref let="i" cid="c19097006.1">
				</crossref>
				The heavens proclaim his righteousness,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				and all 
				<crossref let="j" cid="c19097006.2">
				</crossref>
				the peoples see his glory.
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19097007">
			</marker>
			<verse num="7">
				<begin-line
					>
				</begin-line>
				All worshipers of images are 
				<crossref let="k" cid="c19097007.1">
				</crossref>
				put to shame,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				who make their boast in 
				<crossref let="l" cid="c19097007.2">
				</crossref>
				worthless idols;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				<crossref let="m" cid="c19097007.3">
				</crossref>
				worship him, all you gods!
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v19097008">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="8">
				<begin-line
					>
				</begin-line>
				Zion hears and 
				<crossref let="n" cid="c19097008.1">
				</crossref>
				is glad,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				and the daughters of Judah rejoice,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				because of your judgments, O 
				<span class="divine-name">
					Lord
				</span>
				.
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19097009">
			</marker>
			<verse num="9">
				<begin-line
					>
				</begin-line>
				For you, O 
				<span class="divine-name">
					Lord
				</span>
				, are 
				<crossref let="o" cid="c19097009.1">
				</crossref>
				most high over all the earth;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				you are exalted far above 
				<crossref let="p" cid="c19097009.2">
				</crossref>
				all gods.
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v19097010">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="10">
				<begin-line
					>
				</begin-line>
				O you who love the 
				<span class="divine-name">
					Lord
				</span>
				, 
				<crossref let="q" cid="c19097010.1">
				</crossref>
				hate evil!
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				He 
				<crossref let="r" cid="c19097010.2">
				</crossref>
				preserves the lives of his 
				<crossref let="s" cid="c19097010.3">
				</crossref>
				saints;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				he 
				<crossref let="t" cid="c19097010.4">
				</crossref>
				delivers them from the hand of the wicked.
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19097011">
			</marker>
			<verse num="11">
				<begin-line
					>
				</begin-line>
				<crossref let="u" cid="c19097011.1">
				</crossref>
				Light 
				<crossref let="v" cid="c19097011.2">
				</crossref>
				is sown
				<note nid="n19097011.1">
				</note>
				 for the righteous,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				and joy for the upright in heart.
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19097012">
			</marker>
			<verse num="12">
				<begin-line
					>
				</begin-line>
				<crossref let="w" cid="c19097012.1">
				</crossref>
				Rejoice in the 
				<span class="divine-name">
					Lord
				</span>
				, O you righteous,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				and 
				<crossref let="x" cid="c19097012.2">
				</crossref>
				give thanks to his holy name!
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<end-block-indent
				>
			</end-block-indent>
		</chapter>
	</book>
</crossway-bible>


