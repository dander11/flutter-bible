<?xml version="1.0" encoding="utf-8"?> 
<crossway-bible>
	<book title="Psalms" num="18">
		<marker class="begin-verse" mid="v19099001">
		</marker>
		<chapter num="99">
			<heading
				>
				The 
				<span class="divine-name">
					Lord
				</span>
				 Our God Is Holy
			</heading>
			<begin-block-indent
				>
			</begin-block-indent>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="1">
				<begin-line
					>
				</begin-line>
				<crossref let="s" cid="c19099001.1">
				</crossref>
				The 
				<span class="divine-name">
					Lord
				</span>
				 reigns; 
				<crossref let="t" cid="c19099001.2">
				</crossref>
				let the peoples tremble!
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				He 
				<crossref let="u" cid="c19099001.3">
				</crossref>
				sits enthroned upon the cherubim; 
				<crossref let="v" cid="c19099001.4">
				</crossref>
				let the earth quake!
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19099002">
			</marker>
			<verse num="2">
				<begin-line
					>
				</begin-line>
				The 
				<span class="divine-name">
					Lord
				</span>
				 is 
				<crossref let="w" cid="c19099002.1">
				</crossref>
				great in Zion;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				he is 
				<crossref let="x" cid="c19099002.2">
				</crossref>
				exalted over all the peoples.
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19099003">
			</marker>
			<verse num="3">
				<begin-line
					>
				</begin-line>
				Let them praise your 
				<crossref let="y" cid="c19099003.1">
				</crossref>
				great and awesome name!
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				<crossref let="z" cid="c19099003.2">
				</crossref>
				Holy is he!
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19099004">
			</marker>
			<verse num="4">
				<begin-line
					>
				</begin-line>
				<crossref let="a" cid="c19099004.1">
				</crossref>
				The King in his might 
				<crossref let="b" cid="c19099004.2">
				</crossref>
				loves justice.
				<note nid="n19099004.1">
				</note>
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				You have established equity;
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				you have executed justice
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				and righteousness in Jacob.
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19099005">
			</marker>
			<verse num="5">
				<begin-line
					>
				</begin-line>
				<crossref let="c" cid="c19099005.1">
				</crossref>
				Exalt the 
				<span class="divine-name">
					Lord
				</span>
				 our God;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				<crossref let="d" cid="c19099005.2">
				</crossref>
				worship at his 
				<crossref let="e" cid="c19099005.3">
				</crossref>
				footstool!
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				<crossref let="z" cid="c19099005.4">
				</crossref>
				Holy is he!
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v19099006">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="6">
				<begin-line
					>
				</begin-line>
				<crossref let="f" cid="c19099006.1">
				</crossref>
				Moses and Aaron were among his 
				<crossref let="g" cid="c19099006.2">
				</crossref>
				priests,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				Samuel also was among those who 
				<crossref let="h" cid="c19099006.3">
				</crossref>
				called upon his name.
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				They 
				<crossref let="i" cid="c19099006.4">
				</crossref>
				called to the 
				<span class="divine-name">
					Lord
				</span>
				, and he answered them.
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19099007">
			</marker>
			<verse num="7">
				<begin-line
					>
				</begin-line>
				In 
				<crossref let="j" cid="c19099007.1">
				</crossref>
				the pillar of the cloud he spoke to them;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				they 
				<crossref let="k" cid="c19099007.2">
				</crossref>
				kept his testimonies
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				and the statute that he gave them.
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v19099008">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="8">
				<begin-line
					>
				</begin-line>
				O 
				<span class="divine-name">
					Lord
				</span>
				 our God, you answered them;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				you were 
				<crossref let="l" cid="c19099008.1">
				</crossref>
				a forgiving God to them,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				but 
				<crossref let="m" cid="c19099008.2">
				</crossref>
				an avenger of their wrongdoings.
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19099009">
			</marker>
			<verse num="9">
				<begin-line
					>
				</begin-line>
				Exalt the 
				<span class="divine-name">
					Lord
				</span>
				 our God,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				and worship at his 
				<crossref let="n" cid="c19099009.1">
				</crossref>
				holy mountain;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				for the 
				<span class="divine-name">
					Lord
				</span>
				 our God is holy!
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<end-block-indent
				>
			</end-block-indent>
		</chapter>
	</book>
</crossway-bible>


