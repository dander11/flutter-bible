<?xml version="1.0" encoding="utf-8"?> 
<crossway-bible>
	<book title="Psalms" num="18">
		<marker class="begin-verse" mid="v19146001">
		</marker>
		<chapter num="146">
			<heading
				>
				Put Not Your Trust in Princes
			</heading>
			<begin-block-indent
				>
			</begin-block-indent>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="1">
				<begin-line
					>
				</begin-line>
				<crossref let="n" cid="c19146001.1">
				</crossref>
				Praise the 
				<span class="divine-name">
					Lord
				</span>
				!
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				Praise the 
				<span class="divine-name">
					Lord
				</span>
				, O my soul!
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19146002">
			</marker>
			<verse num="2">
				<begin-line
					>
				</begin-line>
				I will praise the 
				<span class="divine-name">
					Lord
				</span>
				<crossref let="o" cid="c19146002.1">
				</crossref>
				as long as I live;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				<crossref let="p" cid="c19146002.2">
				</crossref>
				I will sing praises to my God while I have my being.
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v19146003">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="3">
				<begin-line
					>
				</begin-line>
				<crossref let="q" cid="c19146003.1">
				</crossref>
				Put not your trust in princes,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				<crossref let="r" cid="c19146003.2">
				</crossref>
				in a son of man, in whom there is 
				<crossref let="s" cid="c19146003.3">
				</crossref>
				no salvation.
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19146004">
			</marker>
			<verse num="4">
				<begin-line
					>
				</begin-line>
				When 
				<crossref let="t" cid="c19146004.1">
				</crossref>
				his breath departs, he returns to the earth;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				on that very day his plans perish.
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v19146005">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="5">
				<begin-line
					>
				</begin-line>
				<crossref let="u" cid="c19146005.1">
				</crossref>
				Blessed is he whose help is the God of Jacob,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				whose 
				<crossref let="v" cid="c19146005.2">
				</crossref>
				hope is in the 
				<span class="divine-name">
					Lord
				</span>
				 his God,
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19146006">
			</marker>
			<verse num="6">
				<begin-line
					>
				</begin-line>
				<crossref let="w" cid="c19146006.1">
				</crossref>
				who made heaven and earth,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				the sea, and all that is in them,
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				<crossref let="x" cid="c19146006.2">
				</crossref>
				who keeps faith forever;
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19146007">
			</marker>
			<verse num="7">
				<begin-line class="indent">
				</begin-line>
				<crossref let="y" cid="c19146007.1">
				</crossref>
				who executes justice for the oppressed,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				<crossref let="z" cid="c19146007.2">
				</crossref>
				who gives food to the hungry.
				<end-line
					>
				</end-line>
				<end-paragraph
					>
				</end-paragraph>
				<begin-paragraph class="line-group">
				</begin-paragraph>
				<begin-line
					>
				</begin-line>
				<crossref let="a" cid="c19146007.3">
				</crossref>
				The 
				<span class="divine-name">
					Lord
				</span>
				 sets the prisoners free;
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19146008">
			</marker>
			<verse num="8">
				<begin-line class="indent">
				</begin-line>
				<crossref let="b" cid="c19146008.1">
				</crossref>
				the 
				<span class="divine-name">
					Lord
				</span>
				 opens the eyes of the blind.
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				<crossref let="c" cid="c19146008.2">
				</crossref>
				The 
				<span class="divine-name">
					Lord
				</span>
				 lifts up those who are bowed down;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				<crossref let="d" cid="c19146008.3">
				</crossref>
				the 
				<span class="divine-name">
					Lord
				</span>
				 loves the righteous.
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19146009">
			</marker>
			<verse num="9">
				<begin-line
					>
				</begin-line>
				<crossref let="e" cid="c19146009.1">
				</crossref>
				The 
				<span class="divine-name">
					Lord
				</span>
				 watches over the sojourners;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				<crossref let="f" cid="c19146009.2">
				</crossref>
				he upholds the widow and the fatherless,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				but 
				<crossref let="g" cid="c19146009.3">
				</crossref>
				the way of the wicked he brings to ruin.
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v19146010">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="10">
				<begin-line
					>
				</begin-line>
				<crossref let="h" cid="c19146010.1">
				</crossref>
				The 
				<span class="divine-name">
					Lord
				</span>
				 will reign forever,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				your God, O Zion, to all generations.
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				<crossref let="n" cid="c19146010.2">
				</crossref>
				Praise the 
				<span class="divine-name">
					Lord
				</span>
				!
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<end-block-indent
				>
			</end-block-indent>
		</chapter>
	</book>
</crossway-bible>


