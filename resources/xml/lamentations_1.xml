<?xml version="1.0" encoding="utf-8"?> 
<crossway-bible>
	<book title="Lamentations" num="25">
		<marker class="begin-verse" mid="v25001001">
		</marker>
		<chapter num="1">
			<heading
				>
				How Lonely Sits the City
			</heading>
			<begin-block-indent
				>
			</begin-block-indent>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="1">
				<begin-line
					>
				</begin-line>
				<crossref let="a" cid="c25001001.1">
				</crossref>
				How lonely sits the city
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				that was full of people!
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				How like 
				<crossref let="b" cid="c25001001.2">
				</crossref>
				a widow has she become,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				she who was great among the nations!
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				She who was 
				<crossref let="c" cid="c25001001.3">
				</crossref>
				a princess among the provinces
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				has become 
				<crossref let="d" cid="c25001001.4">
				</crossref>
				a slave.
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v25001002">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="2">
				<begin-line
					>
				</begin-line>
				<crossref let="e" cid="c25001002.1">
				</crossref>
				She weeps bitterly in the night,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				with tears on her cheeks;
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				<crossref let="f" cid="c25001002.2">
				</crossref>
				among all her lovers
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				she has 
				<crossref let="g" cid="c25001002.3">
				</crossref>
				none to comfort her;
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				<crossref let="h" cid="c25001002.4">
				</crossref>
				all her friends have dealt treacherously with her;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				they have become her enemies.
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v25001003">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="3">
				<begin-line
					>
				</begin-line>
				<crossref let="i" cid="c25001003.1">
				</crossref>
				Judah has gone into exile because of affliction
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				and hard servitude;
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				<crossref let="j" cid="c25001003.2">
				</crossref>
				she dwells now among the nations,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				<crossref let="k" cid="c25001003.3">
				</crossref>
				but finds no resting place;
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				her pursuers have all overtaken her
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				in the midst of her distress.
				<note nid="n25001003.1">
				</note>
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v25001004">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="4">
				<begin-line
					>
				</begin-line>
				The roads to Zion mourn,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				for none come to 
				<crossref let="l" cid="c25001004.1">
				</crossref>
				the festival;
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				<crossref let="m" cid="c25001004.2">
				</crossref>
				all her gates are desolate;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				her priests 
				<crossref let="n" cid="c25001004.3">
				</crossref>
				groan;
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				her virgins have been afflicted,
				<note nid="n25001004.1">
				</note>
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				and she herself suffers bitterly.
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v25001005">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="5">
				<begin-line
					>
				</begin-line>
				<crossref let="o" cid="c25001005.1">
				</crossref>
				Her foes have become the head;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				her 
				<crossref let="p" cid="c25001005.2">
				</crossref>
				enemies prosper,
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				because 
				<crossref let="q" cid="c25001005.3">
				</crossref>
				the 
				<span class="divine-name">
					Lord
				</span>
				 has afflicted her
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				<crossref let="r" cid="c25001005.4">
				</crossref>
				for the multitude of her transgressions;
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				<crossref let="s" cid="c25001005.5">
				</crossref>
				her children have gone away,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				captives before the foe.
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v25001006">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="6">
				<begin-line
					>
				</begin-line>
				From the daughter of Zion
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				all her majesty has departed.
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				Her princes have become like deer
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				<crossref let="t" cid="c25001006.1">
				</crossref>
				that find no pasture;
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				they fled without strength
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				before the pursuer.
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v25001007">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="7">
				<begin-line
					>
				</begin-line>
				Jerusalem remembers
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				in the days of her affliction and wandering
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				<crossref let="u" cid="c25001007.1">
				</crossref>
				all the precious things
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				that were hers from 
				<crossref let="v" cid="c25001007.2">
				</crossref>
				days of old.
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				When her people fell into the hand of the foe,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				and there was none to help her,
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				her foes gloated over her;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				they 
				<crossref let="w" cid="c25001007.3">
				</crossref>
				mocked at her downfall.
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v25001008">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="8">
				<begin-line
					>
				</begin-line>
				<crossref let="x" cid="c25001008.1">
				</crossref>
				Jerusalem sinned grievously;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				therefore she became filthy;
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				all who honored her despise her,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				<crossref let="y" cid="c25001008.2">
				</crossref>
				for they have seen her nakedness;
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				she herself 
				<crossref let="z" cid="c25001008.3">
				</crossref>
				groans
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				and turns her face away.
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v25001009">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="9">
				<begin-line
					>
				</begin-line>
				Her uncleanness was 
				<crossref let="a" cid="c25001009.1">
				</crossref>
				in her skirts;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				<crossref let="b" cid="c25001009.2">
				</crossref>
				she took no thought of her future;
				<note nid="n25001009.1">
				</note>
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				therefore her fall is terrible;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				<crossref let="c" cid="c25001009.3">
				</crossref>
				she has no comforter.
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				<q class="begin-double" qid="" from="" to="">
				</q>
				O 
				<span class="divine-name">
					Lord
				</span>
				, behold my affliction,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				for the enemy has 
				<crossref let="d" cid="c25001009.4">
				</crossref>
				triumphed!
				<q class="end-double" qid="" from="" to="">
				</q>
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v25001010">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="10">
				<begin-line
					>
				</begin-line>
				The enemy has stretched out his hands
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				over all her 
				<crossref let="e" cid="c25001010.1">
				</crossref>
				precious things;
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				for she has seen 
				<crossref let="f" cid="c25001010.2">
				</crossref>
				the nations
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				enter her sanctuary,
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				those whom you 
				<crossref let="g" cid="c25001010.3">
				</crossref>
				forbade
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				to enter your congregation.
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v25001011">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="11">
				<begin-line
					>
				</begin-line>
				All her people 
				<crossref let="z" cid="c25001011.1">
				</crossref>
				groan
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				as 
				<crossref let="h" cid="c25001011.2">
				</crossref>
				they search for bread;
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				they trade their 
				<crossref let="e" cid="c25001011.3">
				</crossref>
				treasures for 
				<crossref let="i" cid="c25001011.4">
				</crossref>
				food
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				to revive their strength.
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				<q class="begin-double" qid="" from="" to="">
				</q>
				Look, O 
				<span class="divine-name">
					Lord
				</span>
				, and see,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				for I am despised.
				<q class="end-double" qid="" from="" to="">
				</q>
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v25001012">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="12">
				<begin-line
					>
				</begin-line>
				<q class="begin-double" qid="" from="" to="">
				</q>
				Is it nothing to you, all 
				<crossref let="j" cid="c25001012.1">
				</crossref>
				you who pass by?
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				<crossref let="k" cid="c25001012.2">
				</crossref>
				Look and see
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				if there is any sorrow like my sorrow,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				which was brought upon me,
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				which 
				<crossref let="l" cid="c25001012.3">
				</crossref>
				the 
				<span class="divine-name">
					Lord
				</span>
				 inflicted
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				on 
				<crossref let="m" cid="c25001012.4">
				</crossref>
				the day of his fierce anger.
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v25001013">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="13">
				<begin-line
					>
				</begin-line>
				<q class="continue-double" qid="" from="" to="">
				</q>
				From on high he 
				<crossref let="n" cid="c25001013.1">
				</crossref>
				sent fire;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				into my bones
				<note nid="n25001013.1">
				</note>
				 he made it descend;
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				<crossref let="o" cid="c25001013.2">
				</crossref>
				he spread a net for my feet;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				he turned me back;
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				<crossref let="p" cid="c25001013.3">
				</crossref>
				he has left me stunned,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				faint all the day long.
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v25001014">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="14">
				<begin-line
					>
				</begin-line>
				<q class="continue-double" qid="" from="" to="">
				</q>
				My transgressions were bound
				<note nid="n25001014.1">
				</note>
				 into 
				<crossref let="q" cid="c25001014.1">
				</crossref>
				a yoke;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				by his hand they were fastened together;
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				they were set upon my neck;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				he caused my strength to fail;
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				the Lord gave me into the hands
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				of those whom I cannot withstand.
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
			<marker class="begin-verse" mid="v25001015">
			</marker>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="15">
				<begin-line
					>
				</begin-line>
				<q class="continue-double" qid="" from="" to="">
				</q>
				The Lord rejected
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				all my mighty men in my midst;
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				he summoned an assembly against me
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				to crush my young men;
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				<crossref let="r" cid="c25001015.1">
				</crossref>
				the Lord has trodden as in a winepress
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				the virgin daughter of Judah.
				<end-line
					>
				</end-line>
			</verse>
			<end-paragraph
				>
			</end-paragraph>
