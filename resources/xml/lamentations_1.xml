				<crossref let="s" cid="c25001016.1">
				</crossref>
				I weep;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				my eyes flow with tears;
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				for 
				<crossref let="t" cid="c25001016.2">
				</crossref>
				a comforter is far from me,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				one to 
				<crossref let="u" cid="c25001016.3">
				</crossref>
				revive my spirit;
				<end-line class="br">
				</end-line>
				<begin-line
					>
				</begin-line>
				my children are desolate,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				for the enemy has prevailed.
				<q class="end-double" qid="" from="" to="">
				</q>
				<end-line
					>
				</end-line>
			</verse>
