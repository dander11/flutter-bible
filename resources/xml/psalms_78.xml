			<begin-block-indent
				>
			</begin-block-indent>
			<begin-paragraph class="line-group">
			</begin-paragraph>
			<verse num="1">
				<begin-line
					>
				</begin-line>
				<crossref let="i" cid="c19078001.2">
				</crossref>
				Give ear, O my people, to my teaching;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				incline your ears to the words of my mouth!
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19078002">
			</marker>
			<verse num="2">
				<begin-line
					>
				</begin-line>
				<crossref let="j" cid="c19078002.1">
				</crossref>
				I will open my mouth 
				<crossref let="k" cid="c19078002.2">
				</crossref>
				in a parable;
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				I will utter dark sayings from of old,
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19078003">
			</marker>
			<verse num="3">
				<begin-line
					>
				</begin-line>
				things that we have heard and known,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
				that our 
				<crossref let="l" cid="c19078003.1">
				</crossref>
				fathers have told us.
				<end-line class="br">
				</end-line>
			</verse>
			<marker class="begin-verse" mid="v19078004">
			</marker>
			<verse num="4">
				<begin-line
					>
				</begin-line>
				We will not 
				<crossref let="m" cid="c19078004.1">
				</crossref>
				hide them from their children,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
