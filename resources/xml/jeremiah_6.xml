			<verse num="10">
				<begin-line
					>
				</begin-line>
				<crossref let="q" cid="c24006010.1">
				</crossref>
				To whom shall I speak and give warning,
				<end-line class="br">
				</end-line>
				<begin-line class="indent">
				</begin-line>
